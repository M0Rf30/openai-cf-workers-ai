{
	"name": "openai-cf-workers-ai",
	"version": "1.0.0",
	"description": "OpenAI-compatible API using Cloudflare Workers AI",
	"private": true,
	"main": "index.js",
	"type": "module",
	"scripts": {
		"deploy": "wrangler deploy index.js",
		"dev": "wrangler dev index.js --local --port 8787",
		"init-dev": "wrangler r2 bucket create ai-images",
		"init-prod": "wrangler r2 bucket create openai-cf-images",
		"start-stackblitz": "WRANGLER_SEND_METRICS=false wrangler dev index.js --remote",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:unit": "vitest run tests/unit",
		"test:integration": "chmod +x tests/integration/*.sh && tests/integration/test_api.sh",
		"test:all": "chmod +x scripts/*.sh && scripts/test_all.sh",
		"test:audio": "chmod +x scripts/*.sh && scripts/test_audio.sh",
		"test:completions": "chmod +x scripts/*.sh && scripts/test_completions.sh",
		"test:embeddings": "chmod +x scripts/*.sh && scripts/test_embeddings.sh",
		"test:images": "chmod +x scripts/*.sh && scripts/test_images.sh",
		"test:models": "chmod +x scripts/*.sh && scripts/test_models.sh",
		"test:rag": "chmod +x scripts/*.sh && scripts/test_rag.sh",
		"test:coverage": "vitest run --coverage",
		"lint": "eslint *.js routes/**/*.js utils/**/*.js",
		"lint:fix": "eslint --fix *.js routes/**/*.js utils/**/*.js",
		"format": "prettier --write \"**/*.{js,json,md}\"",
		"typecheck": "echo 'No TypeScript files to check'",
		"validate": "npm run lint && npm run test:unit"
	},
	"keywords": [
		"openai",
		"cloudflare",
		"workers",
		"ai",
		"api"
	],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@ai-sdk/openai": "^2.0.14",
		"context-optimizer-mcp-server": "^1.0.2",
		"itty-router": "^5.0.18",
		"openai": "^5.12.2"
	},
	"devDependencies": {
		"eslint": "^9.0.0",
		"prettier": "^3.0.0",
		"vitest": "^3.2.4",
		"wrangler": "^4.30.0"
	}
}
